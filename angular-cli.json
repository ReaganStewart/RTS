{
  "project": {
    "version": "1.0.0-beta.26",
    "name": "rts"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
        "styles": [
        "styles/styles.css",
        "styles/imagehover.min.css",
        "styles/bootstrap.css",
        "styles/progress-circle.css",
        "styles/parallax.css",
        "styles/chat-bubbles.css"],
      "scripts": [ "assets/js/fontawesome.js",
                    "assets/js/v4-shims.js",
                    "assets/js/packs/light.js",
                    "assets/js/packs/regular.js",
                    "assets/js/packs/brands.js",
                    "assets/js/packs/solid.js"],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
